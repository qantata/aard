{
  "name": "aard",
  "version": "alpha.2021.1114.0",
  "private": true,
  "bin": "build/server.js",
  "scripts": {
    "dev": "npx ts-node server.ts",
    "dev:w": "npx nodemon server.ts",
    "build": "next build && npx tsc --project server.tsconfig.json",
    "start": "NODE_ENV=production ts-node server.ts",
    "lint": "next lint",
    "p:generate": "npx prisma generate",
    "p:migrate": "npx prisma migrate dev --name init",
    "p:push": "npx prisma db push",
    "p:seed": "npx prisma db seed",
    "p:studio": "npx prisma studio",
    "relay": "relay-compiler --src ./ --schema ./schema.graphql --language typescript --artifactDirectory ./src/__generated__",
    "postinstall": "yarn p:generate"
  },
  "dependencies": {
    "@ctrl/video-filename-parser": "3.x",
    "@fontsource/roboto": "^4.5.1",
    "@mui/icons-material": "^5.1.0",
    "@mui/material": "^5.1.0",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "@mui/styled-engine-sc": "^5.1.0",
    "@prisma/client": "^3.4.1",
    "apollo-server-express": "^3.5.0",
    "babel-plugin-relay": "^12.0.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "graphql": "15.x",
    "klaw": "^4.0.1",
    "next": "12.0.3",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.34.0",
    "react": "^18.0.0-alpha-ee069065d-20211105",
    "react-dom": "^18.0.0-alpha-ee069065d-20211105",
    "react-relay": "^12.0.0",
    "react-router": "5.x",
    "react-router-config": "^5.1.1",
    "relay-runtime": "^12.0.0",
    "styled-components": "^5.3.3",
    "subscriptions-transport-ws": "0.9.19",
    "through2": "^4.0.2",
    "ws": "7.4.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/klaw": "^3.0.3",
    "@types/node": "16.11.6",
    "@types/react": "17.0.34",
    "@types/react-relay": "^11.0.2",
    "@types/react-router-config": "^5.0.3",
    "@types/relay-runtime": "^12.0.0",
    "@types/styled-components": "^5.1.15",
    "@types/through2": "^2.0.36",
    "@types/ws": "7.4.6",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "7",
    "eslint-config-next": "12.0.3",
    "nodemon": "^2.0.14",
    "prisma": "3.4.1",
    "relay-compiler": "^12.0.0",
    "relay-compiler-language-typescript": "^14.3.0",
    "relay-config": "^12.0.0",
    "ts-node": "^10.4.0",
    "typescript": "4.4.4"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "pkg": {
    "targets": ["node16-linux-x64", "node16-win-x64", "node16-macos-x64"]
  }
}
